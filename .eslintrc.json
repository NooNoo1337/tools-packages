{
  /**
  * Берем за основу готовый конфиг от eslint. 
  * Так же расширяем конфиг с помощью prettier, чтобы правила не конфликтовали
  */
  "extends": ["eslint:recommended", "prettier"],
  "plugins": [
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "prettier/prettier": "error", // Обязательно!! Подсвечивает ошибки из Prettier.
    "camelcase": [
      "error"
    ],
    "eqeqeq": [
      "error",
      "always"
    ]
  }
}